@model LogViewer.Models.ViewerModel

@{
    ViewBag.Title = "Logs";
}

<h2>Logs</h2>

<div>Benchmark: @Html.DropDownList("Direction", new SelectList(Model.Benchmarks), new { onchange = "updateTable()", id = "bench" }) </div>

<table id="results" class="table table-striped table-bordered">
    <thead>
        <tr>
            <td>Device</td>
            <td>Data A</td>
            <td>Data B</td>
            <td>Data C</td>
            <td>Data D</td>
        </tr>
    </thead>
</table>

@section Scripts 
{
    <script src="~/Scripts/viewerTable.js" type="text/javascript"></script>
    <script type="text/javascript">
        $(function () {
            updateTable();
        });
    </script>
}